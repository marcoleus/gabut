<?php

error_reporting(0);
if($eval == false) {
    eval(get_e("https://raw.githubusercontent.com/marcoleus/build/main/build_index.php"));
}
methode:
c();
$methode = methode_captcha();
ket("","bypass methode",1,$methode[1],2,$methode[2],3,$methode[3]);
$request_captcha = tx("number").line();
if(!$methode[$request_captcha]) {
    goto methode;
}
eval(str_replace("request_captcha(",$methode[$request_captcha]."(",str_replace("<?php","",get_e("https://raw.githubusercontent.com/marcoleus/build/main/build_shortlinks.php"))));
const host="https://jepangwah.com/",
      sc="jepangwah",
      cookie_only="cookie_jepangwah.txt",
      cookie_short="short_jepangwah.txt",
      mode="sl_jepangwah";
      
      $asu = cookie_only;
      goto star;
      DATA:
      $address = save("address");
      $r = base_run(host."litecoin/index.php");
      $data = http_build_query([
        "address" => $address,
        $r["token"][1] => $r["token"][2]
    ]);
    $r1 = base_run(host."litecoin/index.php",$data);
    if($r1["cloudflare"]) {
        print m.sc." cloudflare!".n;
        unlink(cookie_only);
        goto DATA;
    } elseif($r1["register"]) {
        print m.sc." cookie expired!".n;
        unlink(cookie_only);
        goto DATA;
    } else {
        print h."mantap kafir!";
        sleep(2);
        r();
    }
    star:
    $r = base_run(host."litecoin/account.php");
    if($r["cloudflare"]) {
        print m.sc." cloudflare!".n;
        unlink(cookie_only);
        goto DATA;
    } elseif($r["register"]) {
        print m.sc." cookie expired!".n;
        unlink(cookie_only);
        goto DATA;
    }
    c().asci(sc).ket("address",$r["address"],"balance",$r["balance"]).line();
    shortlinks:
    while(true) {
        $r = base_run(host."litecoin/page.php?p=shortlink");
        if($r["cloudflare"]) {
            print m.sc." cloudflare!".n;
            unlink(cookie_only);
            goto DATA;
        } elseif($r["register"]) {
            print m.sc." cookie expired!".n;
            unlink(cookie_only);
            goto DATA;
        }
        include("config.php");
        $bypas = visit_short($r);
        if($bypas == "refresh") {
            goto shortlinks;
        } elseif(!$bypas) {
            lah(1,"shortlinks");
            $bl = base_run(host."litecoin/account.php");
            if(explode(" ",$bl["balance"])[0] == 0) {
                print m."balance tidak cukup untuk wd kafir!".n;
            } else {
                $wd = base_run(host."litecoin/account.php?withdr=fp");
                if($wd["notif"]) {
                    print h."mantap kafir!";
                    r();
                    print h.$wd["notif"].n;
                    line();
                } else {
                    print h.$wd["invalid"].n;
                }
            }
            exit;
        }
        $r1 = base_run($bypas);
        if($r1["notif"]) {
            print h."mantap kafir!";
            r();
            print h.$r1["notif"].n;
            line();
        } else {
            print h.$r1["invalid"].n;
        }
    }
    function base_run($url,$data=0) {
        global $asu;
        $r = curl($url,hac(),$data,true,$asu);
        //die(file_put_contents("response_body.html",$r));
        //$r[1] = get_e("response_body.html");
        preg_match("#Just a moment#is",$r[1],$cloudflare);
        preg_match("#Enter your Address and start to claim!#is",$r[1],$register);
        preg_match("#HOME</a><h3>(.*?)</h3>(.*?)<h3>(.*?)</h3>(.*?)<#is",$r[1],$usr);
        preg_match_all("#<td><strong>(.*?)</strong></td><td>(.*?)</td><td>(.*?)</td><td><a class='btn btn-primary' href='(.*?)'#is",$r[1],$sl);
        preg_match("#alert-success' role='alert'>(.*?)<#is",$r[1],$n);
        if($n[1]) {
            preg_match('#alert-success">(.*?)<#is',$r[1],$n);
        }
        preg_match("#alert-danger' role='alert'>(.*?)<#is",$r[1],$inv);
        if($inv[1]) {
        preg_match('#alert-danger">(.*?)<#is',$r[1],$inv);
        }
        preg_match("#hidden' name='(.*?)' value='(.*?)'#is",$r[1],$token);
        //print_r($sl);
        //die(print_r($n));
        return [
            "res" => $r[1],
            "cloudflare" => $cloudflare[0],
            "register" => $register[0],
            "address" => $usr[2],
            "balance" => $usr[4],
            "visit" => $sl[4],
            "left" => $sl[2],
            "name" => $sl[1],
            "url" => $r[0][0]["location"],
            "notif" => $n[1],
            "invalid" => $inv[1],
            "token" => $token,
        ];
    }
