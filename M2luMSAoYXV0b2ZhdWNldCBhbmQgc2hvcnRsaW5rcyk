for($i=1;$i<4;$i++){
if($web[$i]){print "\033[1;32m$i \033[1;36m> \033[1;32m".$web[$i]."\n";}}
print "\033[1;32minput number \033[1;36m> \033[1;37m";
$p=preg_replace("/[^0-9]/","",trim(fgets(STDIN)));
$host=$web[$p];
if(!$host){goto go;}


eval(file_get_contents("https://raw.githubusercontent.com/marcoleus/build/main/Public"));
eval(str_replace('name_host',explode(".",$host)[0],str_replace('example',$host,base64_decode(""))));
methode:
$methode=methode_captcha();
